{
   "name": "Notes ",
   "version": "1.2.0",
   "description": "Storage of notes become easier",
   "manifest_version": 2,
   "background": {
     "scripts": [
       "js/background.js"
       ]
   },
   "browser_action": {
      "default_icon": "logo1.png",
      "default_popup": "popup.html",
      "default_title": "Note"
   },
   "icons": {
      "1": "logo1.png",
      "1": "logo1.png"
   },
   "permissions": [ "background", "unlimitedStorage" ]
}


